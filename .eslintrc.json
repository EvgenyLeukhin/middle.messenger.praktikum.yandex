{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "@typescript-eslint/no-unused-vars": "error",
      "indent": ["error", 2, { "SwitchCase": 1 }],
      "no-unused-vars": 2,
      "max-len": [1, 100],
      "max-params": [2, 3]
    }
}
